{% extends 'base.html.twig' %} 
    {% block stylesheets %}
        
        <META HTTP-EQUIV="CACHE-CONTROL" CONTENT="NO-CACHE" /> 
            <meta charset="utf-8">
            <title>{% block title %}Photo{% endblock %}</title>
               
            <meta http-equiv="X-UA-Compatible" content="IE=edge">
            <meta name="viewport" content="width=device-width, initial-scale=1">
            <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
            <!-- Bootstrap -->
            <link href="{{ asset('css/bootstrap.css')}}" rel="stylesheet"/>
            <link rel="stylesheet" href="{{ asset('css/font-awesome.css') }}" />  
            <link rel="stylesheet" href="{{ asset('css/photo_style.css') }}" />
    
 
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
        {% endblock %}
 
        {% block body %}
    
 
    <div class="container">
        <div class="row">
            <div class="add-header">
                <div class="col-md-12">
                    <form  method="POST" action="{{ url('add_photo') }}" id="sign-up" enctype="multipart/form-data">
                        <input class="btn btn-Danger" type="file" name="fileToUpload">
                        <input class="" type="submit" value="Add" name="submit">
                    </form>
                </div>
            </div>
        </div>
    </div>
<div class="container">
    <div class="row">
        
            <div class="col-lg-12">
                <h1 class="title">Photo</h1>
                <hr>
            </div>
                <div class="photo-header1">
                    <div class="col-lg-3 col-md-4 col-xs-6 thumb">
                        <a class="thumbnail" href="#">
                            {% for img in data %}
                                {% if img.getImage is not null %}
                                    <!--<img class="img-responsive" src="{{asset(img.getImage)}}"alt="img">-->
                                    <img class="img-responsive" src="http://placehold.it/400x300" alt="">
                                {% endif %}
                            {%endfor%}
                            <img class="img-responsive" src="http://placehold.it/400x300" alt="">
                        </a>
                        {{ knp_pagination_render(data) }}
                    </div>
                </DIV>
        
    </div>
</div>
         <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="js/bootstrap.js"></script>
            
{%endblock%}